<h1>Add New Entry for <%= @place.name %></h1>
<%= form_with model: [@place, @entry] do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :description %>
  <%= form.text_area :description %>
  <%= form.label :posted_on %>
  <%= form.date_field :posted_on %>
  <%= form.submit %>
<% end %>